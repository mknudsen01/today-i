{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "radix": 0,
    "max-len": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-unresolved": [2, { "ignore": ["^[~]"] }],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": false}],
    "react/forbid-prop-types": 0,
    "quotes": 0,
  }
}
